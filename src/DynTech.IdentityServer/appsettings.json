{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "Authentication": {
    "Google": {
      "clientId": "test",
      "clientSecret": "test"
    },
    "Twitter":{
      "ConsumerKey": "test",
      "ConsumerSecret": "test"
    },
    "Microsoft":{
      "ApplicationId": "Test",
      "Password": "Test"
    }
  },
  "MetricsOptions": {
    "DefaultContextLabel": "Dynamic Tech Profile Service",
    "Enabled": true
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [ 404 ],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,    
    "EnvironmentInfoEndpointEnabled": true
  }
}