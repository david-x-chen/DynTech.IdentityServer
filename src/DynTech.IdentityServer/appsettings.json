{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "WriteTo": [{
          "Name": "RollingFile", 
          "Args": {
            "pathFormat": "logs/logs-{Date}.txt", 
            "fileSizeLimitBytes": "100000000", 
            "rollOnFileSizeLimit": true 
            } 
